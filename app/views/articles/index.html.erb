<p id="notice"><%= notice %></p>
<div class="container" id="page-wrap">
  <h1>Articles</h1>

      <% @articles.each do |article| %>
    <div class="article_entry">
            <h3><%= link_to article.title, article %></h3>
            <i> <%= article.created_at %></i>
            <p><%= sanitize article.body.truncate(300) %></p>
    </div>
      <% end %>
</div>
